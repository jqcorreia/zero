// This is a simple test of function definition and recursion
// Also tests if/then/else statements and variable scope
fn fib(n: i32) -> i32 {
    if n <= 1 {
        return n
    } else {
        return fib(n - 1) + fib(n - 2)
    }
    return 0
}

// Basic loop, variable utation and break statement
fn func() -> i32 {
    a = 1

    for {
        print(a)
        a = a + 1
        if a > 5 {
            break
        } else {
            a = a + 1
        }
    }
    return a
}


fn main() -> i32 {
    print(func()) // Should print "1 3 5" and "6"
    print(fib(10)) // Should print "55"

    return 0
}

